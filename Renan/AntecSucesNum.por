//Atividade 2 Antecessor e Sucessor do número inserido
programa
{
	
	funcao inicio()
	{
	inteiro numero, antecessor, sucessor
		
		escreva ("digite um número: ")
		leia (numero)
		antecessor= numero - 1
		sucessor= numero + 1
		escreva("sucessor= ", sucessor)
		escreva("\nantecessor: ", antecessor)
	}
}